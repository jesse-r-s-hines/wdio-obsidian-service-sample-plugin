{
    "name": "obsidian-sample-plugin",
    "version": "1.0.0",
    "description": "Sample Obsidian plugin using wdio-obsidian-service for e2e tests",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "build-dev": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs development",
        "version": "node version-bump.mjs && git add manifest.json versions.json",
        "lint": "eslint .",
        "test": "run-s test:*",
        "test:unit": "tsc --noEmit && mocha",
        "test:e2e": "wdio run ./wdio.conf.mts",
        "test-android": "wdio run ./wdio.mobile.conf.mts"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@types/chai": "^5.2.3",
        "@types/mocha": "^10.0.10",
        "@types/node": "^18.19.130",
        "@wdio/appium-service": "~9.19.2",
        "@wdio/cli": "~9.19.2",
        "@wdio/globals": "~9.17.0",
        "@wdio/local-runner": "~9.19.2",
        "@wdio/mocha-framework": "~9.19.2",
        "@wdio/spec-reporter": "~9.19.2",
        "appium": "~2.19.0",
        "appium-uiautomator2-driver": "~4.2.9",
        "chai": "^5.3.3",
        "esbuild": "^0.25.12",
        "eslint-plugin-obsidianmd": "^0.1.9",
        "jiti": "^2.6.1",
        "mocha": "^10.8.2",
        "npm-run-all2": "^7.0.2",
        "obsidian": "^1.11.4",
        "obsidian-launcher": "^2.2.0",
        "tslib": "^2.8.1",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.52.0",
        "wdio-obsidian-reporter": "^2.2.0",
        "wdio-obsidian-service": "^2.2.0"
    }
}
